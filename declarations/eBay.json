{
  "name": "eBay",
  "documents": {
    "Imprint": {
      "fetch": "https://www.ebay.ie/pages/help/policies/legal-imprint.html",
      "select": [
        ".HelpContent"
      ],
      "remove": [
        ".anchorTop"
      ]
    },
    "Terms of Service": {
      "fetch": "https://www.ebay.com/help/policies/member-behaviour-policies/user-agreement?id=4259",
      "select": [
        "#mainContent"
      ],
      "remove": [
        ".breadcrumbs",
        "#catalogue",
        ".expand_collapse_list > .title",
        ".article_relate",
        ".contact_us"
      ],
      "executeClientScripts": true
    }
  }
}
